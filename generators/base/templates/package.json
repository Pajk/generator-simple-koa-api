{
  "name": "<%= API_NAME %>",
  "version": "0.1.0",
  "description": "<%= API_NAME %>",
  "main": "src/startup.js",
  "scripts": {
    "test": "echo 'Cleaning DB...' && node db-test-reset.js 1> /dev/null && NODE_ENV=test node test.js $FILE | tap-spec",
    "cover": "echo 'Cleaning DB...' && node db-test-reset.js 1> /dev/null && istanbul cover test.js $FILE | tap-spec",
    "start": "node src/api.js",
    "doc": "aglio -i doc/api.md -o doc/api.html --theme-full-width",
    "lint": "eslint src/ test/ test.js",
    "graph": "dot -Tjpg di_graph.dot -Gsize=16,6! -Gratio=fill -Gdpi=250 -Gfontsize=50 -o di_graph.jpg && open di_graph.jpg",
    "graphsvg": "dot -Tsvg di_graph.dot -o di_graph.svg && open -a 'Google Chrome' di_graph.svg",
    "db-migrate": "db-migrate",
    "db-rollback": "db-rollback",
    "db-new": "db-create",
    "db-status": "db-status"
  },
  "config": {
    "db-migrator": {
      "db-url": "postgresql://localhost?ssl=false",
      "directory": "./migrations"
    }
  },
  "author": "<%= AUTHOR_NAME %> <<%= AUTHOR_EMAIL %>>",
  "repository": {
    "type": "git",
    "url": "https://github.com/YOU/YOURAPI.git"
  },
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "co": "^4.6.0",
    "co-pg": "^2.0.0",
    "co-request": "^1.0.0",
    "credential": "^1.0.0",
    "db-migrator": "^1.0.3",
    "dotenv": "^2.0.0",
    "flat-transform": "^4.1.1",
    "jsonwebtoken": "7.0.0",
    "koa": "^1.2.0",
    "koa-body": "^1.4.0",
    "koa-compress": "^1.0.8",
    "koa-logger": "^1.3.0",
    "koa-route": "^2.4.2",
    "lodash": "^4.11.1",
    "pg": "^4.5.1",
    "pg-bluebird": "^1.0.8",
    "simple-builder": "^2.3.2",
    "thenify": "^3.2.0",
    "uuid": "^2.0.2",
    "validator": "^5.2.0"
  },
  "devDependencies": {
    "bandage-runner": "^1.0.0",
    "co-supertest": "git://github.com/pajk/co-supertest.git#22fb0e691bf5e75504675afeee800e52b5a1f9f2",
    "eslint": "3.0.1",
    "istanbul": "^0.4.0",
    "supertest": "^1.1.0",
    "tap-spec": "^4.1.0"
  },
  "engines": {
    "node": "6.3.0"
  },
  "cacheDirectories": [
    "node_modules"
  ]
}
